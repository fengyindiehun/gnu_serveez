#ifndef __SVZCONFIG_H__
#define __SVZCONFIG_H__ 1

/* Define to 'int' if <winsock.h> does not define. */
#undef SOCKET

/* Define to 'int' if <winsock.h> does not define. */
#undef HANDLE

/* Define as the return type of signal handlers (int or void).  */
#undef RETSIGTYPE

/* Define as __inline if that's what the C compiler calls it.  */
#undef inline

/* Make CygWin / MinGW32 use large FD sets. */
#undef FD_SETSIZE

/* Define if you are using Windows Socket-API (not CYGWIN). */
#undef Win32_Winsock

/* Define for faster code generation. */
#undef WIN32_LEAN_AND_MEAN

/* Define if debug output should be supported. */
#undef ENABLE_DEBUG

/* Define if debug code should be suppressed. */
#undef NDEBUG

/* Define if you have the <varargs.h> header file.  */
#undef HAVE_VARARGS_H

/* Define if you have the snprintf function. */
#undef HAVE_SNPRINTF

/* Define if you have the vsnprintf function. */
#undef HAVE_VSNPRINTF

/* Define if flood protection should be supported. */
#undef ENABLE_FLOOD_PROTECTION

/* Define if the password encryption should be supported. */
#undef ENABLE_CRYPT

/* Define if you have the crypt function. */
#undef HAVE_CRYPT

/* Define to empty if the keyword does not work.  */
#undef const

/* Define to `unsigned' if <sys/types.h> doesn't define.  */
#undef size_t

#endif /* !__SVZCONFIG_H__ */
